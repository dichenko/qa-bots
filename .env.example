# Telegram Bot
# Один бот (для обратной совместимости)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Несколько ботов (после TELEGRAM_BOT_TOKEN_ указывается ID бота, например BOT1, SUPPORT, FEEDBACK)
TELEGRAM_BOT_TOKEN_BOT1=your_first_bot_token
TELEGRAM_BOT_TOKEN_BOT2=your_second_bot_token
TELEGRAM_BOT_TOKEN_SUPPORT=your_support_bot_token

# ID владельца ботов (один для всех ботов)
OWNER_TELEGRAM_ID=your_telegram_id

# Supabase (автоматически предоставляется интеграцией Vercel-Supabase)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_ANON_KEY=your_anon_key
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key

# PostgreSQL Connection (автоматически предоставляется интеграцией Vercel-Supabase)
POSTGRES_URL=your_postgres_url
POSTGRES_PRISMA_URL=your_postgres_prisma_url
POSTGRES_URL_NON_POOLING=your_postgres_url_non_pooling
POSTGRES_USER=your_postgres_user
POSTGRES_HOST=your_postgres_host
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DATABASE=your_postgres_database
SUPABASE_JWT_SECRET=your_jwt_secret 